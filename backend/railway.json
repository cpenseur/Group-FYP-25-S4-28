{
  "$schema": "https://railway.app/railway.schema.json",
  "build": {
    "builder": "NIXPACKS"
  },
  "deploy": {
    "startCommand": "python manage.py collectstatic --noinput && gunicorn config.wsgi --workers 4 --bind 0.0.0.0:$PORT",
    "healthcheckPath": "/api/",
    "healthcheckTimeout": 30
  }
}
